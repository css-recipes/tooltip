{
  "name": "cssrecipes-tooltip",
  "description": "Simple tooltip using an html attribute for the content",
  "version": "0.3.0",
  "main": "index.css",
  "style": "index.css",
  "files": [
    "lib",
    "index.css"
  ],
  "devDependencies": {
    "myth": "^1.0.0"
  },
  "scripts": {
    "setup": "npm install && mkdir -p build",
